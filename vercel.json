{
  "version": 2,
  "name": "informative-blog",
  "env": {
    "REACT_APP_API_URL": "https://informative-blog-web-production-bfbe.up.railway.app"
  },
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
